<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.iu.main.student.StudentDAO">
  
  <select id="getList" resultType="com.iu.main.student.StudentDTO">
  	SELECT ST_NO,ST_NAME FROM STUDENT ORDER BY ST_NO DESC
  </select>
  
  <select id="getDetail" resultType="com.iu.main.student.StudentDTO">
  	SELECT * FROM STUDENT WHERE ST_NO=#{st_no}
  </select>
  	
  	
  </mapper>
  

  
  
  