<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.iu.main.bookComment.BookCommentDAO">
  
  <select id="getList" resultType="BookCommentDTO" parameterType="BookCommentDTO">
  	SELECT *
  	FROM BOOKCOMMENT
  	WHERE BOOKNUM=${bookNum}
  	ORDER BY NUM DESC
  	
  </select>
  </mapper>

